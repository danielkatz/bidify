{
    "defaultSeverity": "warning",
    "extends": ["tslint:recommended"],
    "rules": {
        "object-literal-sort-keys": false,
        "no-console": false,
        "variable-name": {
            "options": [
                "ban-keywords",
                "check-format",
                "allow-pascal-case",
                "allow-leading-underscore"
            ]
        }
    }
}